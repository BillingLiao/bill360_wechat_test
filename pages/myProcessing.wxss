.processing_in {
  box-shadow: 0rpx 0rpx 8rpx 0rpx #edf0f5;
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
}
.myProcessing .myProcessing_ul {
  width: 100%;
  height: 100%;
}
.myProcessing .myProcessing_ul .myProcessing_li {
  width: 100%;
  height: 300rpx;
  position: relative;
}
.myProcessing .myProcessing_ul .myProcessing_li .yellow_icon {
  position: absolute;
  top: 11rpx;
  left: 0rpx;
  width: 15rpx;
  height: 104rpx;
}
.myProcessing .myProcessing_ul .myProcessing_li .yewu_bianhao {
  margin-left: 42rpx;
  height: 120rpx;
  font-weight: 600;
  font-size: 30rpx;
}
.myProcessing .myProcessing_ul .myProcessing_li .dingdan_time {
  float: right;
  margin-right: 31rpx;
  margin-top: 5rpx;
  font-weight: 600;
  height: 120rpx;
  color: #73777c;
  font-size: 24rpx;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
