.person_header {
  width: 100%;
}
.person_header .person_header_img {
  width: 20%;
  display: inline-block;
}
.person_header .person_header_img .img_size {
  border-radius: 50%;
  width: 102rpx;
  vertical-align: bottom;
  height: 102rpx;
  margin-top: 57rpx;
}
.person_header .person_header_font {
  width: 80%;
  color: #fff;
  display: inline-block;
}
.person_header .person_header_font .person_header_font_size {
  text-align: left;
}
.person_section {
  margin-top: 55rpx;
  height: 100%;
  width: 100%;
}
.person_bgc {
  width: 100%;
  padding: 21rpx;
  box-sizing: border-box;
  background-color: #fff;
  height: 480rpx;
  border-radius: 16rpx;
  background-image: url('data:image/png;base64,');
}
.bbb {
  border-bottom: 1rpx solid #dddd;
  padding-bottom: 32rpx;
}
.card_in {
  padding: 21rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.mingpian {
  color: #272727;
  font-size: 30rpx;
  font-weight: 600;
}
.bianji {
  margin-top: 20rpx;
  color: #0061de;
  font-size: 26rpx;
  float: right;
  margin-right: 30rpx;
}
.ccc {
  width: 100%;
  padding-top: 10rpx;
}
.ccc .card_content {
  margin-top: 23rpx;
  width: 15%;
  display: inline-block;
  vertical-align: middle;
  font-size: 24rpx;
  vertical-align: top;
}
.ccc .card_input {
  vertical-align: middle;
  display: inline-block;
  width: 70%;
  padding: 0rpx 5rpx;
  margin-top: 12rpx;
}
.person_footer {
  margin-top: 43rpx;
  border-top: 1px solid #ddd;
}
.person_footer .dingdan {
  padding-top: 41rpx;
  padding-bottom: 37rpx;
  color: #fff;
  font-size: 30rpx;
  border-bottom: 1px solid #ddd;
}
.person_footer .dingdan .right_icon {
  width: 15rpx;
  height: 27rpx;
  float: right;
  margin-top: 10rpx;
}
